{
	"expo": {
		"name": "Kita Rescue",
		"slug": "kitarescue",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"scheme": "myapp",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.kitastack.kitarescue"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		},
		"experiments": {
			"tsconfigPaths": true
		},
		"plugins": [
			"expo-router"
		],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "80421527-97cf-408e-ab85-521dce0e3d16"
			}
		}
	}
}